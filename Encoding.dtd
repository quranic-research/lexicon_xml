<?xml version="1.0" encoding="UTF-8"?>
<!-- Import DTD with the definitions for 'Inline Elements' -->
<!ENTITY % base SYSTEM "Inline.dtd">
    %base;

<!-- Elements for list of characters (listNym) -->
<!ELEMENT charDecl (desc*,char*)>
<!ATTLIST charDecl
  type (arabic|greek|hebrew|syriac) #REQUIRED
>
<!ELEMENT desc (#PCDATA|ref)*>
<!ELEMENT char (charName,desc*,note*,mapping+)>
<!ATTLIST char
  xml:id ID #REQUIRED
>
<!ELEMENT charName (#PCDATA)*>
<!ELEMENT mapping (#PCDATA|ref)*>
<!ATTLIST mapping
  type (standard|internal|buckw|buckw2) #REQUIRED
>

<!-- Elements for list of canonical names (listNym) -->
<!ELEMENT listNym (nym)+>
<!ATTLIST listNym
  type (terms|authorities) #REQUIRED
>
<!ELEMENT nym (etym)>
<!ATTLIST nym
  xml:id ID #REQUIRED
>
<!ELEMENT etym (abbr,expan,note*)>

