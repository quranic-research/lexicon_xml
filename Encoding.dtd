<?xml version="1.0" encoding="UTF-8"?>
<!-- Import DTD with the definitions for 'Inline Elements' -->
<!ENTITY % base SYSTEM "Inline.dtd">
    %base;

<!-- Elements for list of characters (listNym) -->
<!ELEMENT charDecl (desc*,char*)>
<!ATTLIST charDecl
  type (arabic|hebrew|syriac) #REQUIRED
>
<!ELEMENT desc (#PCDATA|ref)*>
<!ELEMENT char (desc*,note*,mapping+)>
<!ATTLIST char
  xml:id ID #REQUIRED
>
<!ELEMENT mapping (#PCDATA|ref)*>
<!ATTLIST mapping
  type (standard|internal|buckwater|buckw2) #REQUIRED
>

<!-- Elements for list of canonical names (listNym) -->
<!ELEMENT listNym (nym)+>
<!ATTLIST listNym
  type (terms|authorities) #REQUIRED
>
<!ELEMENT nym (etym)>
<!ATTLIST nym
  xml:id ID #REQUIRED
>
<!ELEMENT etym (abbr,expan,note*)>

<!-- Define character declarations with all required character data and assign them to ENTITY chars. -->
<!ENTITY characters '
<charDecl type="arabic">
</charDecl>
<charDecl type="hebrew">
</charDecl>
<charDecl type="syriac">
</charDecl>
'>

