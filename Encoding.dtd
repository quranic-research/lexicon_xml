<!-- Define listNym with all gramatical terms and assign them to ENTITY terms. -->
<!ENTITY terms '
<listNym type="terms">
	<nym xml:id="tAccord">
		<etym>
			<abbr>Accord.</abbr>
			<expan resp="EWL">according</expan>
		</etym>
	</nym>
	<nym xml:id="tAccusCase">
		<etym>
			<abbr>Accus. case</abbr>
			<expan resp="EWL">Accusative case, <foreign lang="ar">naSobN</foreign>.</expan>
		</etym>
	</nym>
	<nym xml:id="tAct">
		<etym>
			<abbr>Act.</abbr>
			<expan resp="EWL">Active, <foreign lang="ar">mabonieBN lilofaAEili</foreign> or <foreign lang="ar">lilomaEoluwmi</foreign>.</expan>
		</etym>
	</nym>
	<nym xml:id="tActPartN">
		<etym>
			<abbr>Act. part. n.</abbr>
			<expan resp="EWL">Active participial noun, <foreign lang="ar">Aisomu faAEilK</foreign>.</expan>
		</etym>
	</nym>
	<nym xml:id="tAdvN">
		<etym>
			<abbr>Adv. n.</abbr>
			<expan resp="EWL">Adverbial noun, <foreign lang="ar">ZarofN</foreign>, and sometimes <foreign lang="ar">SifapN</foreign>; of place, <foreign lang="ar">Zarofu makaAnK</foreign>; and of time, <foreign lang="ar">Zarofu zamaAnK</foreign>.</expan>
		</etym>
	</nym>
	<nym xml:id="tAgent">
		<etym>
			<abbr>Agent</abbr>
			<expan resp="EWL">Agent, <foreign lang="ar">faAEilN</foreign>.</expan>
		</etym>
	</nym>
	<nym xml:id="tAnalogous">
		<etym>
			<abbr>Analogous</abbr>
			<expan resp="EWL">Analogous, <ref target="tRegular">or regular</ref>, <foreign lang="ar">qiyaAsieBN</foreign> and <foreign lang="ar">maqiysN</foreign>.</expan>
		</etym>
	</nym>
	<nym xml:id="tAnalogy">
		<etym>
			<abbr>Analogy</abbr>
			<expan resp="EWL">Analogy, <foreign lang="ar">fiyaAsN</foreign>.</expan>
		</etym>
	</nym>
	<nym xml:id="tAnomalous">
		<etym>
			<abbr>Anomalous</abbr>
			<expan resp="EWL">Anomalous <ref target="tIrregular">or irregular</ref>, <foreign lang="ar">gayoru qiyaAsieBK</foreign> and <foreign lang="ar">gayoru maqiysK</foreign> or <foreign lang="ar">XaAcBN</foreign> (<ref target="tDev">see “Dev.”</ref>) or <foreign lang="ar">naAdirN</foreign> (<ref target="tExtr">see “Extr.”</ref>).</expan>
		</etym>
	</nym>
	<nym xml:id="tAor">
		<etym>
			<abbr>Aor.</abbr>
			<expan resp="EWL">Aorist, <foreign lang="ar">muDaAriEN</foreign>.</expan>
		</etym>
	</nym>
	<nym xml:id="tAplastic">
		<etym>
			<abbr>Aplastic</abbr>
			<expan resp="EWL">Aplastic applied to a noun and to a verb, <foreign lang="ar">jaAmidN</foreign>.</expan>
		</etym>
	</nym>
	<nym xml:id="tApp">
		<etym>
