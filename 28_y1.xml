			<div1 type="letter" n="y" xml:id="Letter_y"> 
				<head><foreign lang="ar">I</foreign></head>
				<superEntry type="main" n="5297" xml:id="Root_ytm">
					<dictScrap type="front">
						<title type="root"><foreign lang="ar">ytm</foreign></title>
					</dictScrap>
					<form>
						<orth lang="ar">ytm</orth>
					</form>
					<entry type="main" n="47885" class="verb" xml:id="ytm_4">
						<pb n="3063" xml:id="Page_3063"/>
						<dictScrap>
							<title type="entry">4. ⇒ <foreign lang="ar">Aytm</foreign></title>
						</dictScrap>
						<form>
							<iType>4.</iType>
							<orth lang="ar">Aytm</orth>
						</form>
						<sense n="1" xml:id="ytm_4_A1">
							4. <foreign lang="ar">Oayotamahu</foreign> 
							[<hi>He made him fatherless</hi>]. (TA in art. <foreign lang="ar">Aym</foreign>.) 
							<ref target="Root_Aym">See an ex. in art. <foreign lang="ar">Aym</foreign></ref>, 
							<ref target="Aym_2">conj. 2</ref>.
						</sense>
					</entry>
				</superEntry>
				<superEntry type="main" n="5298" xml:id="Root_ytn">
					<dictScrap type="front">
						<title type="root"><foreign lang="ar">ytn</foreign></title>
					</dictScrap>
