							<orth orig="" extent="full" lang="ar">AljaAru AlyarobuwEiIRu</orth>
						</form>
						<hom n="A">
						<sense n="1">
							<s><foreign lang="ar">AljaAru AlyarobuwEiIRu</foreign>: see <foreign lang="ar">AljAr AlbaraAqiXiIRu</foreign>, in art. <foreign lang="ar">brqX</foreign>.</s>
						</sense>
						</hom>
					</entry>
				</superEntry>
