						<form type="entry">
							<orth orig="" extent="full" lang="ar">Uiyvamu</orth>
							<orth extent="full" lang="ar">c</orth>
						</form>
								<foreign lang="ar">Uiyvamu</foreign> and <foreign lang="ar">tiyvamu</foreign> [for <foreign lang="ar">Mvamu</foreign> and <foreign lang="ar">taOovamu</foreign>]: see <foreign lang="ar">Oavima</foreign>.
					</entry>
				</superEntry>
